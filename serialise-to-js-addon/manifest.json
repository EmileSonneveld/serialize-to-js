{
  "name": "JS Picking",
  "description": "Find the JavaScript variables in the midst of chaos",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "unlimitedStorage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "128": "/images/icon128.png"
  }
}
